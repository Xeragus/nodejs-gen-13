<table class="table">
  <thead>
    <tr>
      <th scope="col">SKU</th>
      <th scope="col">Name</th>
      <th scope="col">Price</th>
    </tr>
  </thead>
  <tbody>
    <% products.forEach(product => { %>
      <tr>
        <td><%= product.sku %></td>
        <td><%= product.name %></td>
        <td>$<%= product.price %></td>
      </tr>
    <% }) %>
  </tbody>
</table>